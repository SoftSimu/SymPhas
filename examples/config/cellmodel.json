{
  "definitions": {
    "DT": 0.025,
    "WIDTH": 1.0,
    "GRID_SIZE": 200,
    "ITERATIONS": 100,
    "CELL_COUNT": 18
  },
  "simulation": {
    "dimension": 2,
    "time_steps": [
      {
        "dt": "${DT}",
        "iterations": "${ITERATIONS}"
      }
    ],
    "save": {
      "type": "DEFAULT",
      "save_initial": true
    },
    "stencil": {
      "order": 2,
      "ptl": 9,
      "ptg": 6,
      "ptb": 13
    },
    "solver_variation": 1
  },
  "model": {
    "name": "CELL_MODEL_NO_MOT",
    "fields": [
      {
        "index": 0,
        "domain": "main_domain",
        "initial_condition": "cell",
        "count": 0,
        "modifier": "plot.default"
      },
      {
        "index": 1,
        "domain": "main_domain",
        "initial_condition": "cell",
        "count": "${CELL_COUNT}",
        "modifier": "plot.max"
      }
    ],
    "coefficients": {
      "c1": 7,
      "c2": 0.5,
      "c3": 49,
      "c4": 10,
      "c5": 1.5e3,
      "c6": 3.75,
      "c7": 1.35,
      "c8": 0,
      "c9": 1e4
    }
  },
  "initial_conditions": {
    "cell": {
      "type": "BUBBLE",
      "tags": [ "FIXED", "RAND", "A", "C" ],
      "parameters": [ "${CELL_COUNT}", 0, 1.0, 0.8486, 2.2 ]
    }
  },
  "intervals": {
    "unit_interval": {
      "points": "${GRID_SIZE}",
      "width": "${WIDTH}"
    },
    "large_domain": {
      "start": -5.0,
      "end": 5.0,
      "points": 256
    }
  },
  "domains": {
    "main_domain": {
      "dimension": 2,
      "intervals": {
        "x": "unit_interval",
        "y": "unit_interval"
      },
      "boundaries": {
        "x": {
          "left": { "type": "PERIODIC" },
          "right": { "type": "PERIODIC" }
        },
        "y": {
          "left": { "type": "PERIODIC" },
          "right": { "type": "PERIODIC" }
        }
      }
    }
  },
  "output": {
    "directory": "./simulations"
  }
}